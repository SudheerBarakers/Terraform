# Terraform Scripts

## vi main.tf

resource "aws_instance" "web" {
  ami           = "ami-06878d265978313ca"
  instance_type = "t2.micro"

  tags = {
    Name = "vm01"
  }
}


## vi provider.tf


terraform {
  required_providers {
    aws = {
      source = "hashicorp/aws"
      version = "4.49.0"
    }
  }
}

provider "aws" {
